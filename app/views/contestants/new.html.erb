<div id="sign_up_form">
<h1>Sign up</h1>
<%= form_for @contestant, :html =>{:multipart => true} do |f| %>
  <%= f.error_messages %>
  <div class="field">
  <p>FILE UPLOAD HERE</p>
  </div>
  <div class="field">
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </div>
  <div class="field">
    <%= f.label :first_name %><br />
    <%= f.text_field :first_name %>
  </div>
  <div class="field">
    <%= f.label :last_name %><br />
    <%= f.text_field :last_name %>
  </div>
  <div class="field">
    <%= f.label :address %><br />
    <%= f.text_field :address %>
  </div>
  <div class="field">
    <%= f.label :city %><br />
    <%= f.text_field :city %>
  </div>
  <div class="field">
    <%= f.label :state %><br />
    <%= f.text_field :state %>
  </div>  
  <div class="field">
    <%= f.label :zip %><br />
    <%= f.text_field :zip %>
  </div>
  <div class="field">
    <%= f.label :phone %><br />
    <%= f.text_field :phone %>
  </div>


 <%= fields_for @contestant.current_entry do |current_entry_form|  %> 
  <div class="field"> 
    <%= current_entry_form.label :photographer %><br />    
    <%= current_entry_form.text_field :photographer %>
  </div>
  <div class="field">
    <%= current_entry_form.label :location %><br />    
    <%= current_entry_form.text_field :location %>
  </div>
  <div class="field">
    <%= current_entry_form.label :photo %><br />
    <%= current_entry_form.file_field :photo %>    
  </div> 
  <div class="field">
    <%= current_entry_form.label :contest_source %><br />
    <%= select(:contestant_entry, :contest_source, 
                [['Email', 'Email'], ['Friend', 'Friend']]
              ) 
    %>
  </div>
 <% end %>
  <div class="field">
  

  <div class="field">
    <%= f.label :agreed_to_rules %><br />
    <%= f.check_box :agreed_to_rules %>
  </div> 

  <div class="actions">
    <%= f.submit "Sign up" %>
  </div>
<% end %>

