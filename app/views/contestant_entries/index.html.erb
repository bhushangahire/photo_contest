<h1>Contest Gallery</h1>
<table>
  <%= will_paginate  @contestants %>
  <% @contestants.each do |contestant| %>
    <tr>
      <td>
          <h3>Contestant Email: <%= contestant.email %></h3></td>
      <td/>
      <td/>  
    </tr>
    <tr>
      <td>
        <% entry = get_entry contestant.contestant_entries,  1%>
        <% if not entry.nil? %>
        <%=  render  :partial => 'entry' , :object => entry%>
      <% end %>
      </td>
      <td>
        <% entry = get_entry contestant.contestant_entries,  2%>
        <% if not entry.nil? %>
        <%=  render  :partial => 'entry' , :object => entry%>
      <% end %>
      </td>
      <td>
        <% entry = get_entry contestant.contestant_entries,  3%>
        <% if not entry.nil? %>
        <%=  render  :partial => 'entry' , :object => entry%>
      <% end %>
      </td> 
    </tr>
  <% end %><!-- end unique contestant loop-->
</table>
