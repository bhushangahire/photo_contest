<%= image_tag entry.photo.url(:medium), :class => "entry_image" %>
<% if entry.rating.nil? %>
  <span class="rating_form">
  <%= form_for entry  do |f| %>    
      <%= f.error_messages %>
      <p class="rating_form_element"><%= f.label :rating %>
      <%= select( :entry, :rating, list_of_numbers(5) ) %></p>
      <%= f.label :month %>
      <%= select( :entry, :rating, list_of_numbers(12) ) %>
      <%= f.submit "Submit Vote" %>
   <% end %><!-- rating form -->
   </span>   
<% else %>
  <span class="rating"><p>Rating: <%=  entry.rating %></p></span>
<% end %><!--was item rated-->
<%= link_to "Details", contestant_entry_path(entry) %>

